{% extends 'base.jinja2' %}

{% block dependencies %}
<link rel="stylesheet" href="{{ url_for('static', filename='css/home.css') }}">
{% endblock %}

{% block content %}

<div class="home">
	<div class="jumbotron">
		<div class="body">
			{{ message }}
		</div>
		<div class="buttons">
			<div class="flexible-space"></div>
			<a class="button button-primary" onclick="window.history.back()">{{ localise('site-go_back') }}</a>
		</div>
	</div>
</div>

{% endblock %}